<div id="header">EMC Scheduler - Philly Office
<span id="menu_links" >
<%= link_to image_tag("blue-button-planner.png",  {class: "header_button", alt: "New Reservation"}) + "New", new_reservation_path, method: :get %>
<%= link_to image_tag("blue-button-doc-checks.png",{class: "header_button", alt: "My Reservations"}) + "Mine", reservations_path(:mine=>"Y"), method: :get %>
<%= link_to image_tag("blue-button-document.png",  {class: "header_button", alt: "All Reservations"}) + "All", reservations_path, method: :get %>
