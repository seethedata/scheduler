        <%= link_to image_tag("blue_button-2-people.png",  {class: "header_button", alt: "All Reservations"}) + "Users", users_path %>
        <%= link_to image_tag("blue_button-gears.png",  {class: "header_button", alt: "All Reservations"}) + "Assets",assets_path %>
	<%= link_to image_tag("blue_button-comp-meter.png",  {class: "header_button", alt: "All Reservations"}) + "Reports", report_reservations_path %>
<% end %>
<%= link_to image_tag("blue_button-migrate.png",  {class: "header_button", alt: "All Reservations"}) + "Logout", signout_path, method: :get %>
</span>
</div>
