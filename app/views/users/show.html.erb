<table id="user_list">
<tr>
<th>Username</th>
<th>First Name</th>
<th>Last Name</th>
<th>Email Address</th>
<th>Active?</th>
</tr>
<tr>
<td><%= @user.login%></td>
<td><%= @user.first_name %></td>
<td><%= @user.last_name %></td>
<td><%= @user.email %></td>
<td><%= @user.active %></td>
<td><%= link_to "Edit", edit_user_path(@user) %></td>
<td><%= link_to "Remove", @user, method: :delete, data: {confirm: "Are you sure?" } %></td>
</tr>
</table>
